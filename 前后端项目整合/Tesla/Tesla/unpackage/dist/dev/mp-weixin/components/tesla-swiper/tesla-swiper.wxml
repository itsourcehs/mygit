<view><view class="swiper-area"><swiper data-event-opts="{{[['change',[['handleChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><image src="{{item.imgUrl}}" mode="aspectFill"></image></view></swiper-item></block></swiper><block wx:for="{{swiperList}}" wx:for-item="car" wx:for-index="index1" wx:key="index1"><block wx:if="{{current===index1}}"><view class="swiper-content"><view class="title">{{''+car.title+''}}</view><view class="meta"><block wx:for="{{car.config}}" wx:for-item="conf" wx:for-index="index2" wx:key="index2"><view class="items"><view class="item-title">{{conf.title}}</view><view class="item-subtitle">{{conf.subtitle}}</view></view></block></view><view class="action"><view class="book-btn"><button data-event-opts="{{[['tap',[['handleBook',['$event']]]]]}}" bindtap="__e">预约试驾</button><view class="book-btn-back"></view></view><view data-event-opts="{{[['tap',[['handleViewConfig',['$event']]]]]}}" class="view-configuration" bindtap="__e">查看配置</view></view></view></block></block><view class="swiper-dots"><block wx:for="{{swiperList}}" wx:for-item="cart" wx:for-index="index3" wx:key="index3"><view class="{{['dot',current!==index3||'active']}}"></view></block></view></view></view>