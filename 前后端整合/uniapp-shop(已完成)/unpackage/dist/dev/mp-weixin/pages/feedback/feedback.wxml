<view><view class="feedback_main"><view style="font-size:24rpx;">问题种类</view><view class="feedback_types"><block wx:for="{{types}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="feedback_types_item" data-id="{{item}}" data-event-opts="{{[['tap',[['handleTypeBtn',['$event']]]]]}}" bindtap="__e">{{item.title}}</view></block><view style="padding:6px 13px;width:30%;"></view></view><view class="feedback_text"><textarea name id cols="30" rows="10" placeholder="请描述你的问题..." placeholder-class="place" data-event-opts="{{[['input',[['handleInput',['$event']]]]]}}" value="{{textContent}}" bindinput="__e"></textarea><view class="feedback_uploadImage"><image src="../../static/upload.png" data-event-opts="{{[['tap',[['handleChooseImg',['$event']]]]]}}" bindtap="__e"></image><block wx:for="{{uploadImages}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view style="position:relative;"><image style="margin-left:10px;" src="{{item}}"></image><block wx:if="{{imageList.length>0}}"><icon type="clear" size="20" color="red" data-index="{{index}}" data-event-opts="{{[['tap',[['handleClose',['$event']]]]]}}" bindtap="__e"></icon></block></view></block></view></view><view data-event-opts="{{[['tap',[['handleSubmit',['$event']]]]]}}" class="btns" bindtap="__e"><button class="{{['btn',inpStatus?'add':'']}}"><view style="margin-left:23px;"><image src="../../static/trues.png"></image><text>提交</text></view></button></view></view></view>